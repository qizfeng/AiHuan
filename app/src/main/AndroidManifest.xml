        <activity
            android:name=".ui.user.EditGenderActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/ActivityDialogStyleBottom" />
        <activity android:name=".ui.user.ForgetPassActivity" />
        <activity android:name=".ui.user.MyAttentionActivity" />
        <activity android:name=".ui.index.GoodsAllActivity" />
        <activity android:name=".ui.user.MessagesActivity" />
        <activity android:name=".ui.user.JoinPartyActivity" />
        <activity android:name=".ui.user.DepositActivity" />
        <activity android:name=".ui.user.OrderListActivity" />
        <activity android:name=".ui.user.OrderDetailActivity"/>
        <activity android:name=".ui.user.AddressListActivity"/>
        <activity android:name=".ui.user.AddAddressActivity"/>
        <activity android:name=".ui.user.ModifyAddressActivity"/>
        <activity android:name=".ui.index.GoodsDetailPartyActivity"/>
